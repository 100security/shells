        bash_i "" "tcp"
        ;;
    2)
        bash_i "base64" "tcp"
        ;;
    3)
        bash_i "url" "tcp"
        ;;
    4)
        bash_i "urlx2" "tcp"
        ;;
    5)
        bash_i "" "udp"
        ;;
    6)
        bash_i "base64" "udp"
        ;;
    7)
        bash_i "url" "udp"
        ;;
    8)
