elif [[ $1 == "urlx2" ]] 
then
    rev=$(urlencodeme "$rev")
    rev=$(urlencodeme "$rev")

elif [[ $1 == "base64" ]] 
then
    rev=$(echo -n $rev|$benc --base64url -w0)
fi
if [[ $OS == "Darwin" ]]
then
   echo -n $rev | pbcopy
else 
    echo -n $rev | xclip -sel c
fi
listen "$2"
}

